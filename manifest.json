{
  "manifest_version": 2,

  "name": "I See Code",
  "description": "This extension will turn the page dark and the text green",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
 ],
 "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "css": ["styles.css"],
     "js": ["popup.js"]
   }
 ]
}
